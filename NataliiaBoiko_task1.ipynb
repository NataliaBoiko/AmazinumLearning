   "source": [
    "import numpy as np\n",
    "\n",
    "all_points = 100000\n",
    "circle_points = 0\n",
    "\n",
    "for i in range(all_points):\n",
    "    x = np.random.uniform(-1, 1)\n",
    "    y = np.random.uniform(-1, 1)\n",
    "    if  x**2 + y**2 <= 1:\n",
    "        circle_points += 1\n",
    "\n",
    "pi = 4 * circle_points / all_points\n",
    "\n",
    "print(\"Pi=\", pi)"
   ]
